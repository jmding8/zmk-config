        release-after-ms = <20000>; // 20 seconds
    };

    combos {
        compatible = "zmk,combos";
        // Mode switching combos:
        combo_sys {
            timeout-ms = <LH_COMBO_TIMEOUT>;
            key-positions = <0 12 36>;
            bindings = <&sl SYS>;
        };
//        combo_standard and combo_game are disabled in favor of word combos.
